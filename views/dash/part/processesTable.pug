mixin processesTable(title, id)
    .widget.col-6.p-2
        div
            .head
                a(href="/dash/"+id+"lvl2")
                    .title= title
                
            .body
                table(style="width:100%" id=id)
                    thead 
                        tr 
                            th Process 
                            th Quantity
                            th Percent